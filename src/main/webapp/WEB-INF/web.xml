<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
   

    <servlet>
        <servlet-name>Lab2Servlet</servlet-name>
        <servlet-class>knu.fit.ist.ta.Lab2Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletInitializer</servlet-name>
        <servlet-class>knu.fit.ist.ta.ServletInitializer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Lab2Servlet</servlet-name>
        <url-pattern>/lab2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletInitializer</servlet-name>
        <url-pattern>/</url-pattern>
        
        
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
